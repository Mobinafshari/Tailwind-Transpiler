{
  "name": "tailwind-transpiler",
  "version": "0.0.27",
  "description": "A tool to parse and replace Tailwind CSS class names using AST.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "extract": "node index.js"
  },
  "bin": {
    "extract-classes": "./dist/index.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "tailwind",
    "transpiler",
    "extract-classes",
    "custom-classes"
  ],
  "author": "Mobin Afshari",
  "license": "ISC",
  "dependencies": {
    "@babel/generator": "^7.26.9",
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "fs-extra": "^11.3.0",
    "path": "^0.12.7",
    "rollup": "^4.34.9"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0"
  }
}
